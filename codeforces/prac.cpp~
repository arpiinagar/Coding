#include <bits/stdc++.h>
using namespace std;
using ll = long long int;
typedef pair<int, int> pi;
typedef vector<int> vi;
typedef vector<ll> vl;
typedef vector<string> vs;
#define inv(v)         \
    for (auto &it : v) \
        cin >> it;
#define MOD 1000000007

void solve()
{
	int n;
	cin >> n;
	vi a(n);
	for(auto &x : a) cin >> x;
	sort(a.begin(),a.end());
	reverse(a.begin(),a.end());
	for(auto x: a) cout << x << " ";
	cout << endl;
}
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t;
    cin >> t;
    while(t--)
        solve();
    return 0;
}

